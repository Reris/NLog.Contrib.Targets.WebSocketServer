{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "LogListener": {
    "LogInternals": true,
    "Tcp": [
      {
        "Ip": "v4",
        "Port": "4505",
        "Formats": [
          "json",
          "log4jxml"
        ]
      },
      {
        "Ip": "v6",
        "Port": "4505",
        "Formats": [
          "json"
        ]
      }
    ]
  }
}
